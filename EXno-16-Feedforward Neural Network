import math

# ğŸ”¢ Sigmoid activation function
def sigmoid(x):
    return 1 / (1 + math.exp(-x))

# ğŸ” Derivative of sigmoid (for training, not used here)
def sigmoid_derivative(x):
    return x * (1 - x)

# ğŸš€ Forward pass through the network
def feedforward(x1, x2):
    # ğŸ¯ Input to hidden layer (2 neurons)
    hidden1_input = x1 * 0.1 + x2 * 0.2
    hidden2_input = x1 * 0.3 + x2 * 0.4

    # ğŸ”¥ Apply activation
    hidden1_output = sigmoid(hidden1_input)
    hidden2_output = sigmoid(hidden2_input)

    # ğŸ”„ Hidden to output layer
    output_input = hidden1_output * 0.5 + hidden2_output * 0.6
    output = sigmoid(output_input)

    return output

# ğŸ“¥ Input from user
x1 = float(input("Enter input x1: "))
x2 = float(input("Enter input x2: "))

# âœ… Run the feedforward network
output = feedforward(x1, x2)

# ğŸ‰ Show result
print("\nğŸ” Output of Feedforward Neural Network:", round(output, 4))
